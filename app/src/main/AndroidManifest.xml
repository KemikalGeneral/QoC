<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.endorphinapps.kemikal.queenofclean">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddRecords.AddCustomer" />
        <activity android:name=".AddRecords.AddEmployee" />
        <activity android:name=".AddRecords.AddJob" />
        <activity android:name=".ViewAlls.ViewCustomers" />
        <activity android:name=".ViewAlls.ViewEmployees" />
        <activity android:name=".ViewAlls.ViewJobs" />
        <activity android:name=".DetailViews.DetailCustomer" />
        <activity android:name=".DetailViews.DetailEmployee" />
        <activity android:name=".DetailViews.DetailJob"></activity>
    </application>

</manifest>